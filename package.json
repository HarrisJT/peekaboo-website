{
  "name": "peekaboo-website",
  "description": "Website for Peekaboo Occupation TherapyLLC",
  "version": "1.0.0",
  "author": "Elliot Braem <braemelliot@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm i",
    "lint": "eslint --ignore-path .gitignore .",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server:dev": "nodemon server/index.js",
    "dev": "set DEBUG=app:* && concurrently \"npm run server:dev\" \"npm run client\"",
    "build": "webpack -p",
    "server:prod": "node server/index.js",
    "prod": "set DEBUG=app:* && concurrently \"npm run server:prod\" \"npm run build\""
  },
  "dependencies": {
    "bootstrap": "^4.1.0",
    "compression": "^1.7.3",
    "concurrently": "^3.6.1",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "morgan": "^1.9.0",
    "pug": "^2.0.3",
    "vue": "^2.5.11",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.11",
    "eslint": "^5.3.0",
    "eslint-plugin-vue": "^5.0.0-beta.2",
    "file-loader": "^1.1.11",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.2",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "vue-loader": "^15.3.0",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "^2.22.3"
  }
}
